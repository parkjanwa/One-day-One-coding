i = 15

while i > 0:
  print (i ** 2)       //제곱
  i -= 2
  
  
  
  
i = 1

while i <= 3:
  print (i)            //단순히 i만 출력
  i += 1
else:
  print("That is it.")
  
  
  
  
i = 5
while i>0:
  print(i**2)
  i -= 1
  
  
  
  
i = int(input())
digits = 0
while i > 0:
  i = i//10
  digits += 1
print(digits)            //10으로 몇 번 나누어 지는지 계산후 그 수 출력





i = 1
while i <= 10:
  print(i)
  i += 1
else:
  print("That is it as i became", i)
  print("That is it as i became{}".format(i))
  
