박은진 = [19980414, 'F', '경기도', '파주시']
print(박은진[1:])

nums = [1, 2, 3, 4, 5, 6, 7, 8, 9]
print("홀수: ", nums[::2])
print("짝수: ", nums[1::2])
print("역방향: ", nums[::-1])

interest = ['삼성전자', '크레프톤', 'Never', '카카오', '넥슨', '엔씨소프트']
print(interest[-1])
print(" ".join(interest))
print("/".join(interest))
print("\n".join(interest))

string = "삼성전자/네이버/카카오"
print(string.split("/"))

data = [2, 3, 6, 7, 8, 1, 0]
print(sorted(data))
